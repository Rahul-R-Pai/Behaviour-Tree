<?xml version="1.0"?>
<root main_tree_to_execute="MainTree">
    <!-- ////////// -->
    <BehaviorTree ID="MainTree">
        <Sequence name="OperationCycle">
            <Action ID="MoveLift" target="800"/>
            <Action ID="MoveTurntable" target="90"/>
            <Action ID="MoveTelescope" target="1400"/>
            <Action ID="MoveFork" target="45"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="MoveFork">
            <inout_port name="target"/>
        </Action>
        <Action ID="MoveLift">
            <inout_port name="target"/>
        </Action>
        <Action ID="MoveTelescope">
            <inout_port name="target"/>
        </Action>
        <Action ID="MoveTurntable">
            <inout_port name="target"/>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>
